{
    "version": 2,
    "builds": [
      { "src": "server.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/", "dest": "/" },
      { "src": "/api/query", "dest": "/api/query" },
      { "src": "/public/script.js", "dest": "/server.js" },
      { "src": "/server.js", "dest": "/public/message.json" }
    ]
  }
  